<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout3"
    android:layout_width="410dp"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/item_home_meeting_imageview_image"
        android:layout_width="400dp"
        android:layout_height="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/linear_background"
        android:scaleType="centerCrop"
        />

    <TextView
        android:id="@+id/item_home_meeting_textview_title"
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand"
        android:maxLines="2"
        android:text="Sample"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/item_home_meeting_imageview_image"
        app:layout_constraintEnd_toEndOf="@+id/item_home_meeting_imageview_image"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/item_home_meeting_imageview_image"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.379" />

</androidx.constraintlayout.widget.ConstraintLayout>
